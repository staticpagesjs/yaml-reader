{
  "name": "@static-pages/yaml-reader",
  "version": "0.1.6",
  "description": "YAML reader for static pages.",
  "type": "module",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "browser": "./esm/index.js",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "default": "./esm/index.js"
    }
  },
  "scripts": {
    "prepack": "npm run build",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --outDir cjs --module commonjs",
    "watch:esm": "tsc --watch",
    "watch:cjs": "tsc --watch --outDir cjs --module commonjs",
    "test": "eslint src && npm run build && jest",
    "clean": "rimraf esm cjs/*.?s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/staticpagesjs/yaml-reader.git"
  },
  "keywords": [
    "yaml",
    "static-pages",
    "reader"
  ],
  "author": "László BULIK",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/staticpagesjs/yaml-reader/issues"
  },
  "homepage": "https://staticpagesjs.github.io/",
  "dependencies": {
    "@static-pages/incremental": "0.0.1",
    "glob": "^7.1.7",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/js-yaml": "^4.0.3",
    "@types/node": "^12.20.33",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.2.0",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  }
}
